{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "suppressImplicitAnyIndexErrors": true,
    "jsx": "react"
  },
  "exclude": [
    "node_modules/",
    "src/worker/"
  ]
}
